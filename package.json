{
  "name": "saas-dash",
  "version": "1.0.0",
  "description": "A modern, production-ready SaaS dashboard template with Next.js, TypeScript, Better Auth, and Docker",
  "author": "SaaS Dashboard Starter Contributors",
  "license": "MIT",
  "keywords": [
    "nextjs",
    "saas",
    "dashboard",
    "typescript",
    "docker",
    "postgresql",
    "drizzle",
    "better-auth",
    "shadcn-ui",
    "tailwind"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/iDevEngineer/saas-dash.git"
  },
  "bugs": {
    "url": "https://github.com/iDevEngineer/saas-dash/issues"
  },
  "homepage": "https://github.com/iDevEngineer/saas-dash#readme",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "type-check": "tsc --noEmit",
    "test": "vitest",
